{
    "name": "taketool/feuserlist",
    "type": "typo3-cms-extension",
    "description": "Creates a list of frontendusers and groups in the frontend. Fork from sc_feuserlist by Sebastion Christoph.",
    "authors": [
        {
            "name": "Martin Keller",
            "email": "martin.keller@taketool.de",
            "role": "Developer"
        },
        {
            "name": "Sebastian Christoph",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^12.4.2 || ^12.4.99"
    },
    "autoload": {
        "psr-4": {
            "Taketool\\Feuserlist\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/feuserlist": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "feuserlist"
        }
    }
}
